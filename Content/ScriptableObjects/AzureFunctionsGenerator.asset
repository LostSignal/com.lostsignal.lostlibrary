  text: "// <auto-generated/>\n#pragma warning disable\n\nusing System;\nusing Lost.CloudFunctions;\nusing
    Microsoft.AspNetCore.Http;\nusing Microsoft.Extensions.Logging;\nusing UnityEngine;\n\npublic
    sealed class AzureFunctionWrapper : IDisposable\n{\n    private ILogger log;\n\n   
    public AzureFunctionWrapper(FunctionExecutionContext req, HttpRequest httpRequest,
    ILogger log, out CloudFunctionContext context)\n    {\n        this.Initialize(\n           
    req?.TitleAuthenticationContext,\n            req?.CallerEntityProfile?.Lineage?.MasterPlayerAccountId,\n           
    req?.FunctionArgument,\n            httpRequest,\n            log,\n           
    out context);\n    }\n\n    public AzureFunctionWrapper(PlaystreamExecutionContext
    req, HttpRequest httpRequest, ILogger log, out CloudFunctionContext context)\n   
    {\n        this.Initialize(\n            req?.TitleAuthenticationContext,\n           
    null,\n            null,\n            httpRequest,\n            log,\n           
    out context);\n    }\n\n    private void Initialize(TitleAuthenticationContext
    titleAuthenticationContext, string playerId, string functionArgument, HttpRequest
    httpRequest, ILogger log, out CloudFunctionContext context)\n    {\n        Debug.OnLog
    += OnLog;\n        Debug.OnLogWarning += OnLogWaning;\n        Debug.OnLogError
    += OnLogError;\n        Debug.OnLogAssert += OnLogAssert;\n        Debug.OnLogException
    += OnLogException;\n\n        string titleId = titleAuthenticationContext?.Id;\n\n       
    global::PlayFab.PlayFabSettings.staticSettings.TitleId = Environment.GetEnvironmentVariable(\"PF_TITLE_ID\",
    EnvironmentVariableTarget.Process);\n        global::PlayFab.PlayFabSettings.staticSettings.DeveloperSecretKey
    = Environment.GetEnvironmentVariable(\"PF_SECRET_KEY\", EnvironmentVariableTarget.Process);\n\n       
    this.log = log;\n        this.log.LogInformation($\"Title Id: {titleId}\");\n       
    this.log.LogInformation($\"Player Id: {playerId}\");\n        this.log.LogInformation($\"Function
    Argument: {functionArgument}\");\n\n        context = new CloudFunctionContext\n       
    {\n            PlayerId = playerId,\n            TitleAuthenticationContext =
    new PlayFab.PlayFabAuthenticationContext\n            {\n                EntityId
    = titleId,\n                EntityToken = titleAuthenticationContext.EntityToken,\n               
    EntityType = \"title\",\n            }\n        };\n    }\n\n    public void
    Dispose()\n    {\n        Debug.OnLog -= OnLog;\n        Debug.OnLogWarning -=
    OnLogWaning;\n        Debug.OnLogError -= OnLogError;\n        Debug.OnLogAssert
    -= OnLogAssert;\n        Debug.OnLogException -= OnLogException;\n    }\n\n   
    private void OnLog(string message)\n    {\n        this.log.Log(LogLevel.Information,
    $\"Info: {message}\");\n    }\n\n    private void OnLogWaning(string message)\n   
    {\n        this.log.Log(LogLevel.Warning, $\"Warning: {message}\");\n    }\n\n   
    private void OnLogError(string message)\n    {\n        this.log.Log(LogLevel.Error,
    $\"Error: {message}\");\n    }\n\n    private void OnLogAssert(string message)\n   
    {\n        this.log.Log(LogLevel.Error, $\"Assert: {message}\");\n    }\n\n   
    private void OnLogException(Exception ex)\n    {\n        this.log.Log(LogLevel.Error,
    $\"Exception: {ex}\");\n    }\n}\n"
